<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(bio) -%>">
  <span class='title'>
    <%= bio.Name %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), bio_url(bio),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_bio_path(bio),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_bio_path(bio),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => bio.Name),
        :method => :delete %>
  </span>
</li>
